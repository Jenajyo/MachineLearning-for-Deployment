    "def myreduce(func,mylist):\n",
    "    if len(mylist)==1:\n",
    "        return mylist[0]\n",
    "    result=mylist[0]\n",
    "    for i in range(0,len(mylist)-1):\n",
    "        result=func(result,mylist[i+1])\n",
    "    return result"
   ]
  },
